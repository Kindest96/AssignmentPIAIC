   "source": [
    "decimal = int(input(\"Enter a decimal number: \"))\n",
    "temporary_decimal = decimal\n",
    "binary = 0\n",
    "multiplier = 1\n",
    "\n",
    "while temporary_decimal>=0:\n",
    "    binary+=((temporary_decimal%2)*multiplier)\n",
    "    temporary_decimal//=2\n",
    "    multiplier*=10\n",
    "\n",
    "print(\"Binary Representation of\", decimal, \"is\", binary)"
   ]
